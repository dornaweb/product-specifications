{
    "name": "amiut/product-specifications",
    "description": "Wordpress plugin to create specification tables",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
      {
        "name": "Dornaweb",
        "email": "info@dornaweb.com",
        "homepage": "https://www.dornaweb.com",
        "role": "company"
      },
      {
        "name": "Amin A. Rezapour",
        "email": "hi@amin.nz",
        "homepage": "https://amin.nz"
      }
    ],
    "require": {
      "php": ">=7.2"
    },
    "require-dev": {
        "inpsyde/php-coding-standards": "^1.0",
        "inpsyde/wp-stubs": "dev-main",
        "johnpbloch/wordpress-core": "^6.1",
        "squizlabs/php_codesniffer": "^3.7.2",
        "vimeo/psalm": "^5.8.0"
    },
    "autoload": {
      "psr-4": {
        "Amiut\\ProductSpecs\\": "src/"
      }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      }
    ],
    "extra": {
      "installer-disable": true
    },
    "config": {
      "allow-plugins": {
        "dealerdirect/phpcodesniffer-composer-installer": true
      },
      "optimize-autoloader": true,
      "sort-packages": true
    }
  }
