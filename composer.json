{
    "name": "amiut/product-specifications",
    "description": "Wordpress plugin to create specification tables",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
      {
        "name": "Dornaweb",
        "email": "info@dornaweb.com",
        "homepage": "https://www.dornaweb.com",
        "role": "company"
      },
      {
        "name": "Amin A. Rezapour",
        "email": "hi@amin.nz",
        "homepage": "https://amin.nz"
      }
    ],
    "require": {
      "php": ">=7.4"
    },
    "require-dev": {
        "automattic/phpcs-neutron-standard": "^1.7",
        "automattic/vipwpcs": "^2.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "johnpbloch/wordpress-core": "^6.2",
        "phpcompatibility/php-compatibility": "^9.3",
        "squizlabs/php_codesniffer": "^3.7",
        "suin/phpcs-psr4-sniff": "^3.0",
        "vimeo/psalm": "^5.9",
        "wp-cli/wp-cli": "^2.7",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "Amiut\\ProductSpecs\\": "src/"
      }
    },
    "repositories": [
      {
        "type": "composer",
        "url": "https://wpackagist.org"
      }
    ],
    "extra": {
      "installer-disable": true
    },
    "config": {
      "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
      "optimize-autoloader": true,
      "sort-packages": true
    }
  }
